<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#bd9797"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
<android.support.v7.widget.GridLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#bd9797"
    tools:layout_editor_absoluteX="60dp"
    tools:layout_editor_absoluteY="60dp">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="@string/java"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:fontFamily="casual"
        app:layout_column="1"
        app:layout_row="0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        app:layout_column="0"
        app:layout_row="1"
        tools:ignore="contentDescription"
        app:srcCompat="@drawable/gjava1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp"
        app:layout_column="1"
        app:layout_row="1"
        app:srcCompat="@drawable/gjava2"
        tools:ignore="contentDescription" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        app:layout_column="2"
        app:layout_row="1"
        app:srcCompat="@drawable/gjava3"
        tools:ignore="contentDescription" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="@string/xml"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:fontFamily="casual"
        app:layout_column="1"
        app:layout_row="2" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        app:layout_column="0"
        app:layout_row="3"
        app:srcCompat="@drawable/gxml1"
        tools:ignore="contentDescription" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        app:layout_column="2"
        app:layout_row="3"
        app:srcCompat="@drawable/gxml2"
        tools:ignore="contentDescription" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        app:layout_column="0"
        app:layout_row="4"
        app:srcCompat="@drawable/gxml3"
        tools:ignore="contentDescription" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        app:layout_column="2"
        app:layout_row="4"
        app:srcCompat="@drawable/gxml4"
        tools:ignore="contentDescription" />

    <ImageView
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        app:layout_column="1"
        app:layout_row="3"
        app:srcCompat="@drawable/and"
        tools:ignore="contentDescription" />

    <ImageView
        android:layout_width="127dp"
        android:layout_height="153dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        app:layout_column="1"
        app:layout_row="4"
        app:srcCompat="@drawable/abc"
        tools:ignore="contentDescription" />
</android.support.v7.widget.GridLayout>
</ScrollView>